{
  "search.exclude": {
    "**/target": true
  },
  "rust-analyzer.linkedProjects": [
    "rental-dvd/app/Cargo.toml",
    "rental-dvd/backend/Cargo.toml",
    "rental-dvd/entity/Cargo.toml",
    "rental-dvd/frontend/Cargo.toml"
  ],
  "lldb.verboseLogging": true,
  "lldb.launch.initCommands": [
    "settings set target.disable-aslr false"
  ],
  "tailwindCSS.includeLanguages": {
    "rust": "html",
    "*rs": "html"
  },
  "files.associations": {
    "*rs": "rust"
  },
  "editor.quickSuggestions": {
    "other": "on",
    "comments": "on",
    "strings": true
  },
  "css.validate": false
}
